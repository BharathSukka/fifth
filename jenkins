pipeline{
        agent any
        stages{
            stage('git'){
                steps{
                    echo 'git'
                    sh 'git init'
                    sh 'git pull https://github.com/BharathSukka/fifth.git"
                    sh 'ls'
                    }
                    }
                    stage('Build'){
                    steps{
                    echo 'lets build a code'
                    sh 'python3 jungle.py'
                    }
                    }
                    stage('test'){
                    steps{
                    echo 'lets test the code'
                    }
                    }
                    }
                    }

